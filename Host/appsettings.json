{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReleaseOptions": {
    "SaveLastReleases": 3
  },
  "MockDataSettings": {
    "AddProjects" :  false
  },
  "CacheSettings": {
    "UseDistributedCache": false,
    "PreferRedis": false,
    "RedisURL": "localhost:6379",
    "ExpTimeMin": 10
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "ConnectionString": "Data Source=Projects.Aggregator.db", //"server=localhost;database=Projects.Aggregator.database;Trusted_Connection=true",
    "Provider": "Sqlite" //SQLServer, PostgreSQL, Sqlite
  },
  "SecuritySettings": {
    "RootUserEmail": "belzebbuz@mail.ru",
    "DefaultPassword": "qwert1234QW",
    "RequireConfirmedAccount": false,
    "JwtSettings": {
      "Key": "cewiubqehncqoiy98q326yr8q27bfuwhbecwuiqbciqwjecbmkqcnq9we8yr",
      "ExpirationInDays": 3
    }
  },
  "MiddlewareSettings": {
    "EnableHttpsLogging": true,
    "EnableLocalization": true
  },
  "Serilog": {
    "Using": [],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithHangfireContext"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Hangfire": "Warning",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    },
    "Properties": {
      "Application": "Projects.Aggregator"
    },
    "WriteTo": [
      {
        "Args": {
          "path": "Logs/logs.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information",
          "retainedFileCountLimit": 5
        },
        "Name": "File"
      }
    ]
  }
}
